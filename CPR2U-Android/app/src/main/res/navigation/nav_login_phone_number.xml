<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_login_phone_number"
    app:startDestination="@id/loginPhoneNumberFragment">

    <fragment
        android:id="@+id/loginPhoneNumberFragment"
        android:name="com.example.cpr2u_android.presentation.auth.LoginPhoneNumberFragment"
        android:label="LoginPhoneNumberFragment"
        tools:layout="@layout/fragment_login_phone_number">
        <action
            android:id="@+id/action_loginPhoneNumberFragment_to_loginPhoneNumberCheckFragment"
            app:destination="@id/loginPhoneNumberCheckFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginPhoneNumberCheckFragment"
        android:name="com.example.cpr2u_android.presentation.auth.LoginPhoneNumberCheckFragment"
        android:label="LoginPhoneNumberCheckFragment"
        tools:layout="@layout/fragment_login_phone_number_check" >
        <action
            android:id="@+id/action_loginPhoneNumberCheckFragment_to_signUpNickNameFragment"
            app:destination="@id/signUpNickNameFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpNickNameFragment"
        android:name="com.example.cpr2u_android.presentation.auth.SignUpNickNameFragment"
        tools:layout="@layout/fragment_sign_up_nick_name"
        android:label="SignUpNickNameFragment" >
        <action
            android:id="@+id/action_signUpNickNameFragment_to_signUpAddressFragment"
            app:destination="@id/signUpAddressFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpAddressFragment"
        android:name="com.example.cpr2u_android.presentation.auth.SignUpAddressFragment"
        tools:layout="@layout/fragment_sign_up_address"
        android:label="SignUpAddressFragment" />
</navigation>